 <message>
 <name>messageB</name>
 <description></description>
 <variables>
 <variable><type>int</type><name>messageB_a</name><description></description></variable>
 <variable><type>int</type><name>messageB_b</name><description></description></variable>
 <variable><type>int</type><name>messageB_c</name><description></description></variable>
 <variable><type>int</type><name>messageB_d</name><description></description></variable>
 </variables>
 </message>
 
 </messages>
</xmodel>
